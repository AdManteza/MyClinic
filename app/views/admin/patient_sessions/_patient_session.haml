%tbody
  %tr(scope="row")
    %td= patient_session.id
    %td= patient_session.date
    %td= patient_session.start_time
    %td= patient_session.end_time
    %td= edit_patient_path(patient_session) if patient_session.patient
    %td= edit_appointment_path(patient_session.appointment) if patient_session.appointment
